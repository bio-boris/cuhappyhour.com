create table deals(
deal_id INTEGER PRIMARY KEY AUTOINCREMENT,
venue_id INTEGER,
deal_name TEXT,
deal_day INTEGER,
deal_start TEXT,
deal_end TEXT,
deal_allday TEXT,
deal_description TEXT,
dt_id INTEGER,
)

create table deal_types(
dt_id INTEGER PRIMARY KEY AUTOINCREMENT,
dt_category TEXT,
dt_items TEXT
)


